{
   "name": "nasdrovia-backend",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "release": "npx -p @semantic-release/changelog@^5.0.0 -p @semantic-release/git@^9.0.0  -p semantic-release@^17.0.0 semantic-release --no-ci --debug",
      "commit": "cz",
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
      "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
      "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -f ormconfig",
      "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
      "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "prepare": "cd .. && husky install nazdrovia-backend/.husky"
   },
   "dependencies": {
      "@nestjs/common": "^8.2.2",
      "@nestjs/config": "^1.1.0",
      "@nestjs/core": "^8.2.2",
      "@nestjs/jwt": "^8.0.0",
      "@nestjs/mapped-types": "1.0.0",
      "@nestjs/passport": "^8.0.1",
      "@nestjs/platform-express": "^8.2.2",
      "@nestjs/swagger": "^5.1.5",
      "@nestjs/terminus": "^8.0.1",
      "@nestjs/typeorm": "^8.0.2",
      "class-transformer": "^0.5.0",
      "class-validator": "^0.13.2",
      "cookie-parser": "^1.4.6",
      "dotenv": "^10.0.0",
      "express-rate-limit": "^5.5.1",
      "express-session": "^1.17.2",
      "helmet": "^4.6.0",
      "joi": "^17.4.2",
      "morgan": "^1.10.0",
      "nestjs-rbac": "https://github.com/MassimoTambu/nestjs-rbac",
      "passport": "^0.5.0",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "pg": "^8.7.1",
      "rimraf": "^3.0.2",
      "rxjs": "^7.4.0",
      "swagger-ui-express": "^4.1.6",
      "typeorm": "^0.2.41",
      "uuid": "^8.3.2",
      "winston": "^3.3.3",
      "winston-daily-rotate-file": "^4.5.5",
      "reflect-metadata": "^0.1.13"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.1.5",
      "@nestjs/schematics": "^8.0.5",
      "@nestjs/testing": "^8.2.2",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.13",
      "@types/express-rate-limit": "^5.1.3",
      "@types/express-session": "^1.17.4",
      "@types/jest": "^27.0.3",
      "@types/morgan": "^1.9.3",
      "@types/node": "^16.11.9",
      "@types/passport-jwt": "^3.0.6",
      "@types/passport-local": "^1.0.34",
      "@types/supertest": "^2.0.11",
      "@types/uuid": "^8.3.3",
      "@typescript-eslint/eslint-plugin": "^5.4.0",
      "@typescript-eslint/parser": "^5.4.0",
      "commitizen": "^4.2.4",
      "eslint": "^8.3.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-prettier": "^4.0.0",
      "husky": "^7.0.4",
      "jest": "^27.3.1",
      "lint-staged": "^12.1.1",
      "prettier": "^2.4.1",
      "supertest": "^6.1.6",
      "ts-jest": "^27.0.7",
      "ts-loader": "^9.2.3",
      "ts-node": "^10.4.0",
      "tsconfig-paths": "^3.12.0",
      "typescript": "^4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   },
   "lint-staged": {
      "*.@(ts)": [
         "eslint --fix"
      ],
      "*.js": "eslint --cache --fix"
   },
   "config": {
      "commitizen": {
         "path": "./node_modules/cz-conventional-changelog",
         "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
      }
   }
}
